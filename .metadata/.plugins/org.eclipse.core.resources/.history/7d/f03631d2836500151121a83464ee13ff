
public class indexOfTester {
	public static void main(String[] args) {
		SimpleLinkedList<Integer> testList = new SimpleLinkedList<Integer>();
		Integer desiredO = new Integer(5);
		
		for (int x=0; x<3; x++ ){
			Integer newInt = (Integer) x;
			testList.add(newInt);
		}
		System.out.println(testList);
		
		System.out.println("The # is at "+ testList.indexOf(desiredO));
	}
}
